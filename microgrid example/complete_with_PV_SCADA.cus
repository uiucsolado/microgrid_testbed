        elif (Home_ESS_connection_state == 1) and (NG_genset_connection_state == 1) and (EV_connection_state == 0):
            P_ref_each_kW = (P_import_export - P_load_kW)/2.0
            P_ref_HESS_kW = P_ref_each_kW
            P_ref_NG_kW =-P_ref_each_kW
            P_ref_EV_kW = 0
            
