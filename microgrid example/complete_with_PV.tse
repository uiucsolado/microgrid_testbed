    normal_sample_k1 = std*std_normal_sample_k1 + avg;
    
    //update samples
    std_uniform_sample_k1 = std_uniform_sample;
    std_normal_sample_k1 = std_normal_sample;
    out1 = normal_sample;
    out2 = normal_sample_k1;
}
else
{
    out1 = 0;
    out2 = 0;
}
