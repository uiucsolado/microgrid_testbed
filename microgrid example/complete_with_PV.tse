    //x = (x>>16) ; //right shift to keep upper sixteen bits
    //x = (uint)(two_32) >> 16 ;
    x = (x*x);
    x += (w += s);
    x = (x >> 16)|(x << 16);
    out = ((real)x)/two_32; //normalize value to between 0 and 1
}
else
