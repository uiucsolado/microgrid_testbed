                                in_terminal_properties = "int ok_df;int ok_dv;int ok_dtheta;real df;real dv;real dtheta;real enable_synchronizer;"
                                init_fnc = "/*Begin code section*/
speed_throttle_integral_k1 = 0;
voltage_throttle_integral_k1 = 0;
